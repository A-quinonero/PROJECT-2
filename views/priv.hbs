
<form class="wish-creation-btn text-center">
<div class=" text-center d-flex justify-content-center align-items-center">
<div class="botones-priv ">
  <button class="privBtn btn btn-light text-light m-2" type="button" data-toggle="collapse" data-target="#collapseExample" aria-expanded="false" aria-controls="collapseExample">
   See Wishes
  </button>
  <button class="privBtn btn btn-light text-light m-2" type="button" data-toggle="collapse" data-target="#collapseExample2" aria-expanded="false" aria-controls="collapseExample">
    See Creations
  </button>
</div>
<div class=" profileSettings  text-center d-flex justify-content-center align-items-center">
<img class="imgProf" src="{{myUserHave.imgPath}}" alt="">
<p class="text-light textProfile">Hi <b>{{myUserHave.username}}</b>!</p>
</div>
</div>
</form>

<h1 class="title3 text-center">Wishes</h1>
<div class="" id="collapseExample">
  <div class="containerd text-center">
{{#each myUserWant.wantList}}
    <div class="card" style="width: 18rem;">
  <img src= {{this.imgPath}} class="card-img-top" alt="...">
  <div class="card-body">
    <h5 class="card-title">{{this.title}}</h5>
    <p class="card-text"><i>{{this.category}}</i></p>
    <a class="" href="/priv/delete-want/{{this._id}}"><img class="bin" src="/images/trash-alt-solid.png" srcset=""></a>
  </div>
</div>
{{/each}}
{{#unless myUserWant.wantList}}
    <p class="text-light">¿What do you want?</p>
{{/unless}}
</div>
<br>
<form class="d-flex justify-content-center align-items-center text-center" action="/priv/discover">
<button class="btn btn-light text-light">Add</button>
</form>
</div>
<h1 class="title3 text-center">Creations</h1>
<form class="d-flex justify-content-center align-items-center text-center" action="/priv/create-have">
<button class="btn btn-light text-light">Add Some</button>
</form>

<div class="collapse creations" id="collapseExample2">
  <div class="containerd text-center">
{{#each myUserHave.haveList}}
    <div class="card" style="width: 18rem;">
  <img src= {{this.imgPath}} class="card-img-top" alt="...">
  <div class="card-body">
    <h5 class="card-title">{{this.title}}</h5>
    <p class="card-text"><i>{{this.category}}</i></p>
    <a href="/priv/delete-have/{{this._id}}" class=""><img class="bin" src="/images/trash-alt-solid.png" srcset=""></a>
  </div>
</div>
{{/each}}
{{#unless myUserHave.haveList}}
    <p class="text-light">¿Something to Upload?</p>
{{/unless}}
</div>
<br>

</div>