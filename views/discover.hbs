<form action="/priv/categories" method="post">
<div class="container text-center d-flex justify-content-center align-items-center filtro">
 <select class=" form-control filtro mr-3" name="category" type="text" class="form-control text-center" id="formGroupExampleInput3" placeholder="category"> 
        <option value="drawings">Drawings</option>
        <option value="handmade">Handmade</option>
        <option value="wood">Wood</option>
        <option value="photography">Photography</option>
</select>
<button class="btn btn-go btn-light text-light" type="submit">Go</button>
</div>
</form>

<div class="containerd text-center row">
{{#each products}}
