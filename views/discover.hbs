      <a href="/priv/details/{{id}}"><img  src={{imgPath}} class="card-img-top imagen" alt="..."></a>
      <div  class="card-body ">
        <a href="/priv/details/{{id}}"><h5 class="card-title text-light"><i>{{title}}</i></h5></a>
         
      </div>
    </div>
    {{/each}}
   </div>
 </div>